                    <then>
                        <assign variable="n2" expression="n2 + 1"/>
                        <assign variable="n3" expression="n3 + n1"/>
                    </then>
                    <else/>
                </if>
            </do>
            <output expression="&quot;A m&#233;dia da altura das pessoas com mais de 50 anos &#233;:&quot; &amp; n3 / n2" newline="True"/>
