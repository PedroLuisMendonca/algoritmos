    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="n, n2" type="Integer" array="False" size=""/>
            <declare name="n1, n3" type="Real" array="False" size=""/>
            <assign variable="n2" expression="0"/>
            <assign variable="n3" expression="0"/>
            <do expression="n &gt; 0">
                <output expression="&quot;Digite a idade &quot;" newline="True"/>
